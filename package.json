{
  "name": "prisma-ts-app",
  "version": "1.0.0",
  "description": "for prisma ORM Learnings",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "generate": "npx prisma generate --schema ./prisma-db-1/schema.prisma && npx prisma generate --schema ./prisma-db-2/schema.prisma",
    "migrate": "npx prisma migrate dev --schema ./prisma-db-1/schema.prisma && npx prisma migrate dev --schema ./prisma-db-2/schema.prisma",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Node.js",
    "TypeScript",
    "Prisma",
    "ORM"
  ],
  "author": "Jirayu Sangmai",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-mariadb": "^7.3.0",
    "@prisma/adapter-mssql": "^7.3.0",
    "@prisma/client": "^6.19.2",
    "@types/cors": "^2.8.19",
    "@types/morgan": "^1.9.10",
    "cors": "^2.8.6",
    "express": "^4.19.2",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mssql": "^9.1.9",
    "@types/node": "^20.19.31",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.4",
    "prisma": "^6.19.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
